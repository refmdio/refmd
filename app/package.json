{
  "name": "app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && vite build --config vite.wc.config.ts && tsc",
    "build:wc": "vite build --config vite.wc.config.ts",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint --ext .ts,.tsx src",
    "gen:openapi": "bash -lc 'cd ../api && mkdir -p openapi && cargo run --quiet --bin export-openapi > openapi/openapi.json'",
    "gen:client": "openapi -i ../api/openapi/openapi.json -o src/shared/api/client --client fetch --useOptions --exportCore true --exportServices true --exportModels true",
    "gen:api": "npm run gen:openapi && npm run gen:client"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-query": "^5.51.21",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/router-plugin": "^1.121.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.476.0",
    "monaco-editor": "^0.52.2",
    "monaco-markdown": "^0.0.12",
    "monaco-vim": "^0.4.2",
    "morphdom": "^2.7.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "state-local": "^1.0.7",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "y-indexeddb": "^9.0.12",
